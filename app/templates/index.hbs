<div class="tp-service-area-two p-relative black-bg pt-125 pb-145 z-index-1">
    <div class="container">
       <div class="row">
          <div class="col-xl-12">
             <div class="tp-service-section-box-two text-center">
                <h5 class="tp-subtitle pb-10">Team Members</h5>
                <h2 class="tp-title tp-white-text pb-35">My Members </h2>
             </div>
          </div>
       </div>
       <div class="row gx-12">
       {{#each data as |item|}}
          <div class="col-xl-4  col-lg-4 col-md-6 wow tpfadeUp" data-wow-duration=".3s" data-wow-delay=".6s" style="visibility: visible; animation-duration: 0.3s; animation-delay: 0.6s; animation-name: tpfadeUp;">
             <div class="tp-sv-item-two tp-sv-blue-color mb-30">
                <div class="tp-sv-img mb-40">
                   <img src="assets/img/service/service-3.png" alt="">
                </div>
                <div class="tp-sv-content-tow">
                   <h3 class="tp-sv-product-design">{{item.name}}</h3>
                   <p>{{item.description}}</p>
                </div>
                <div class="tp-sv-icon-two">
                   <LinkTo @route="team-member" @model={{item.id}}>
                      <i class="fas fa-long-arrow-up"></i>
                   </LinkTo>
                </div>
             </div>
          </div>
       {{/each}}
       </div>
    </div>
 </div>